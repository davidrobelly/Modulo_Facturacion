	<queryString>
		<![CDATA[SELECT
     producto.`idproducto` AS producto_idproducto,
     producto.`categoria` AS producto_categoria,
     producto.`nombre` AS producto_nombre,
     producto.`marca` AS producto_marca,
     producto.`modelo` AS producto_modelo,
     producto.`valor` AS producto_valor,
     producto.`minimo` AS producto_minimo,
     producto.`maximo` AS producto_maximo,
     producto.`existencia` AS producto_existencia,
     producto.`estado` AS producto_estado,
     detalle_producto.`iddetalle_producto` AS detalle_producto_iddetalle_producto,
     detalle_producto.`idproducto` AS detalle_producto_idproducto,
     detalle_producto.`numero_serie` AS detalle_producto_numero_serie,
     detalle_producto.`color` AS detalle_producto_color,
     detalle_producto.`estado` AS detalle_producto_estado
FROM
     `producto` producto INNER JOIN `detalle_producto` detalle_producto ON producto.`idproducto` = detalle_producto.`idproducto`
ORDER BY
     Categoria ASC]]>
